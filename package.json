{
  "name": "gzip-loader",
  "version": "0.0.1",
  "description": "gzip loader module for webpack",
  "license": "MIT",
  "repository": "webpack-contrib/gzip-loader",
  "author": "John-David Dalton <john.david.dalton@gmail.com> (http://allyoucanleet.com/)",
  "homepage": "https://github.com/webpack-contrib/gzip-loader",
  "bugs": "https://github.com/webpack-contrib/gzip-loader/issues",
  "main": "index.js",
  "engines": {
    "node": ">= 6.9.0"
  },
  "scripts": {
    "commitlint": "commitlint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint --cache src test",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "lint-staged": "lint-staged",
    "release": "standard-version",
    "release:ci": "conventional-github-releaser -p angular",
    "release:validate": "commitlint --from=$(git describe --tags --abbrev=0) --to=$(git rev-parse HEAD)",
    "security": "nsp check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "ci:lint": "npm run lint && npm run security",
    "ci:test": "npm run test -- --runInBand",
    "ci:coverage": "npm run test:coverage -- --runInBand",
    "defaults": "webpack-defaults"
  },
  "files": [
    "index.js"
  ],
  "peerDependencies": {
    "webpack": "^4.3.0"
  },
  "dependencies": {
    "loader-utils": "^1.1.0",
    "schema-utils": "^0.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "@webpack-contrib/eslint-config-webpack": "^2.0.2",
    "babel-cli": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "conventional-github-releaser": "^2.0.2",
    "cross-env": "^4.0.0",
    "del": "^3.0.0",
    "del-cli": "^0.2.1",
    "eslint": "^3.19.0",
    "eslint-config-webpack": "^1.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.0",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "standard-version": "^4.0.0",
    "webpack": "^2.3.3",
    "webpack-defaults": "^2.1.1"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
